config_version=5

[application]

config/name="2023_11_13_Colonization_Conquerors"
config/description="A game that replicates the gameplay of the classic 1982 game, Utopia."
run/main_scene="res://Interface/Setup_Menu.tscn"
config/features=PackedStringArray("4.1", "GL Compatibility")
run/max_fps=30
boot_splash/bg_color=Color(1, 1, 1, 1)
boot_splash/image="res://Textures/cc-splash-screen.png"
config/icon="res://Textures/cc-icon.png"
config/windows_native_icon="res://cc-icon.ico"
boot_splash/minimum_display_time=5000

[autoload]

Global="*res://Scripts/Global.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3
mouse_cursor/custom_image="res://Textures/cc-mouse-pointer.png"

[editor_plugins]

enabled=PackedStringArray("res://addons/gdUnit4/plugin.cfg")

[filesystem]

import/blender/enabled=false

[gui]

theme/snap_controls_to_pixels=true

